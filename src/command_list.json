{
	"ping": {
		"alias": [
			"p"
		],
		"desc": [
			"usage: {PREFIX}ping {ARGS}",
			"Ping the bot, returns time in milliseconds"
		],
		"admin": "false",
		"on": "message",
		"args": {}
	},
	"avatar": {
		"alias": [
			"ava"
		],
		"desc": [
			"usage: {PREFIX}avatar {ARGS}",
			"returns a user or your avatar URL"
		],
		"admin": "false",
		"on": "message",
		"args": {
			"target":"[@mention or user ID]"
		}
	},
	"uptime": {
		"alias": [
			"up",
			"upt"
		],
		"desc": [
			"usage: {PREFIX}uptime {ARGS}",
			"Returns the uptime of the bot"
		],
		"admin": "false",
		"on": "message",
		"args": {}
	},
	"help": {
		"alias": [
			"h"
		],
		"desc": [
		 	"usage: {PREFIX}help {ARGS}",
		 	"Returns help message of all commands or of a specific command"
		],
		"admin": "false",
		"on": "message",
		"args": {
			"command": "[command name]"
		}
	}
}
