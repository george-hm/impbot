{
	"ping": {
		"alias": [
			"p"
		],
		"desc": [
			"{PREFIX}ping {ARGS}",
			"Ping the bot, returns time in milliseconds"
		],
		"admin": "false",
		"on": "message",
		"args": {}
	},
	"avatar": {
		"alias": [
			"ava"
		],
		"desc": [
			"{PREFIX}avatar {ARGS}",
			"returns a user or your avatar URL"
		],
		"admin": "false",
		"on": "message",
		"args": {
			"target":"[@mention_or_user ID]"
		}
	},
	"uptime": {
		"alias": [
			"up",
			"upt"
		],
		"desc": [
			"{PREFIX}uptime {ARGS}",
			"Returns the uptime of the bot"
		],
		"admin": "false",
		"on": "message",
		"args": {}
	},
	"help": {
		"alias": [
			"h"
		],
		"desc": [
		 	"{PREFIX}help {ARGS}",
		 	"Returns help message of all commands or of a specific command"
		],
		"admin": "false",
		"on": "message",
		"args": {
			"command": "[command_name]"
		}
	},
	"eval": {
		"alias": [
			"ev",
			"eva"
		],
		"desc": [
			"{PREFIX}eval {ARGS}",
			"Runs the JavaScript eval command [DANGEROUS]"
		],
		"admin": "true",
		"on": "message",
		"args": {
			"code": "code_to_execute"
		}
	},
	"botsend": {
		"alias": [
			"sendasbot"
		],
		"desc": [
			"{PREFIX}botsend {ARGS}",
			"Send a message to a channel or user as the bot"
		],
		"admin": "true",
		"on": "message",
		"args": {
			"target": "target",
			"message": "message"
		}
	}
}
